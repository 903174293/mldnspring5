	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.3.xsd">
	<bean id="hiredateObj" class="java.util.Date"/>
	<bean id="deptObj" class="cn.mldn.mldnspring.di.vo.Dept" p:deptno="10" p:dname="财务部" p:enable="on"/>
	<bean id="emp" class="cn.mldn.mldnspring.di.vo.Emp" p:empno="7369" p:ename="SMITH" p:salary="800.00" p:dept-ref="deptObj" p:hiredate-ref="hiredateObj"/>
</beans>
